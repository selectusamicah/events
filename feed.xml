---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
  {% for post in site.categories.events reversed %}
  <item>
    <title>{{ post.title | xml_escape }}</title>
    <link>{{ post.url | prepend: site.url }}</link>
    <startDate>{{ post.date-display-start | xml_escape }}</startDate>
    <endDate>{{ post.date-display-end | xml_escape }}</endDate>
    <location>{{ post.location-value | xml_escape }}</location>
    <eventType>{{ post.location-type | xml_escape }}</eventType>
  </item>
    {% endfor %}
  </channel>
</rss>
